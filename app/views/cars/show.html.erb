 <div class="card" style="width: 18rem;">
  <img src="<%= @car.image_one %>" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= @car.model %></h5>
    <p class="card-text"><%= @car.address %></p>
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item"><%= @car.year %></li>
    <li class="list-group-item"><%= @car.colour %></li>
    <li class="list-group-item"><%= @user.username %></li>
  </ul>
  <div class="card-body">
    <a href="#" class="card-link"></a>
    <a href="#" class="card-link"><%= link_to 'book car', new_car_booking_path(@car) %></a>
  </div>
</div>
